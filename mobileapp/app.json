{
  "expo": {
    "name": "FixKart",
    "slug": "mobileapp",
    "scheme": "fixkart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo1.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B5AA3"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "FixKart uses your location to track sales visits during the work day.",
        "NSLocationAlwaysUsageDescription": "FixKart uses your location to track sales visits during the work day.",
        "NSLocationWhenInUseUsageDescription": "FixKart uses your location to track sales visits during the work day.",
        "UIBackgroundModes": [
          "location"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "usesCleartextTraffic": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F6FAFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fixkart.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c1f493ae-36da-469c-acb3-1c9aba884ac0"
      }
    },
    "plugins": [
      "expo-location"
    ]
  }
}
